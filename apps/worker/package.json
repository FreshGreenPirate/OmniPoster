{
  "name": "worker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "vitest run",
    "format": "prettier --write 'src/**/*.{ts,tsx,json}'"
  },
  "dependencies": {
    "@omniposter/core": "workspace:*",
    "@omniposter/providers": "workspace:*",
    "bullmq": "4.11.2",
    "ioredis": "5.3.2",
    "pino": "8.16.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "tsup": "7.2.0",
    "tsx": "4.7.1",
    "typescript": "5.4.5",
    "vitest": "1.4.0"
  }
}
